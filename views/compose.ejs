<%- include("partials/header") %>

<h1>Compose a Post</h1>

<form class="container" action="compose" method="post">
  <div class="form-group">
    <label for="">Title</label>
    <input class="form-control" type="text" name="postTitle" >
    <label for="">Post</label>
    <textarea class="form-control" name="postText" rows="6" cols="80"></textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="publishBtn">Publish</button>
</form>

<%- include("partials/footer") %>
